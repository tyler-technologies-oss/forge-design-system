{
    "compilerOptions": {
        "incremental": true,
        "target": "ES2022",
        "module": "nodenext",
        "lib": [
            "ES2022"
        ],
        "noEmit": true,
        "outDir": "dist",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "types": [
            "cookie"
        ]
    },
    "include": [
        "./*.ts",
    ]
}